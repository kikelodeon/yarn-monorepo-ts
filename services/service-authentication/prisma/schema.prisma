// services/service-authentication/prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  phone     String?
  createdAt DateTime @default(now())
  deletedAt DateTime?
}
